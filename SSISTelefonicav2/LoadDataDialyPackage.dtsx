#region Namespaces
using System;
using System.Data;
using Microsoft.SqlServer.Dts.Runtime;
using System.Windows.Forms;
using System.Linq;
using System.Diagnostics;
using System.IO;
using System.Text;
using System.Collections.Generic;
#endregion

namespace ST_482c072fa9e94409ad392831bf509422
{
    [Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
    public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    {
        public void Main()
        {
            try
            {
                String connectionString = (String)Dts.Variables["User::connectionString"].Value;
                String remoteFilesString = (String)Dts.Variables["User::remoteFiles"].Value;
                String remoteDir = (String)Dts.Variables["User::remoteDir"].Value;
                int timeOut = (int)Dts.Variables["User::timeOut"].Value;
                DateTime watchTimeFile1 = DateTime.Now;
                DateTime watchTimeFile2 = DateTime.Now;

                var splitedRemoteFilesString = remoteFilesString.Split(',').ToList();

                var remoteFiles = splitedRemoteFilesString
                    .Select(str => String.Format("{0}{1}{2}",
                    remoteDir, remoteDir.EndsWith("/") ? "" : "/",
                    str.Trim().Replace("$$$", DateTime.Now.ToString("ddMM"))));

                while (!remoteFiles.All(remoteFile => StatRemoteFile(connectionString, remoteFile,
                                                                     ref watchTimeFile1, ref watchTimeFile2)))
                {
                    System.Threading.Thread.Sleep(timeOut);
                }
                
                Dts.Variables["User::watchTimeFile1"].Value = watchTimeFile1;
                Dts.Variables["User::watchTimeFile2"].Value = watchTimeFile2;
                Dts.Variables["User::emailBody"].Value = GetBody(splitedRemoteFilesString, watchTimeFile1, watchTimeFile2);
                Dts.Variables["User::emailSubject"].Value = "Visualización";
                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
                try
                {
                    using (StreamWriter sw = File.AppendText(@"C:\SSIS_Configuration\LoadDataDailyPackage.log"))
                    {
                        sw.WriteLine(String.Format("{0} Check for existence error: {1}", DateTime.Now.ToString(@"hh\:mm\:ss\:fff"), ex.Message));
                    }

                    Dts.TaskResult = (int)ScriptResults.Success;
                }
                catch (Exception ex1)
                {
                    Dts.TaskResult = (int)ScriptResults.Failure;
                }
            }
        }

        private string GetBody(List<string> splitedRemoteFilesString, DateTime watchTimeFile1, DateTime watchTimeFile2)
        {
            var emailFormatFile = @"C:\SSIS_Configuration\notification1.html";
            var emailFormat = File.ReadAllText(emailFormatFile, Encoding.Default);

            var emailBody = String.Format(emailFormat,
                            splitedRemoteFilesString[0],
                            splitedRemoteFilesString[1],
                            splitedRemoteFilesString[0].Contains("TRAN_DEUDA") ? watchTimeFile1 : watchTimeFile2,
                            splitedRemoteFilesString[0].Contains("MASTER_SERVICIO") ? watchTimeFile2 : watchTimeFile1
                            );

            return emailBody;
        }

        static Boolean StatRemoteFile(String connetionStr, String remoteFile,
                                      ref DateTime watchTimeFile1, ref DateTime watchTimeFile2)
        {
            var proc = new Process
            {
                StartInfo = new ProcessStartInfo
                {
                    FileName = "winscp.com",
