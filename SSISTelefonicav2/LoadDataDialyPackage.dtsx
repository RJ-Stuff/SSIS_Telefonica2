            Encoding="UTF8"><![CDATA[#region Help:  Introduction to the script task
/* The Script Task allows you to perform virtually any operation that can be accomplished in
 * a .Net application within the context of an Integration Services control flow. 
 * 
 * Expand the other regions which have "Help" prefixes for examples of specific ways to use
 * Integration Services features within this script task. */
#endregion


#region Namespaces
using System;
using System.Data;
using Microsoft.SqlServer.Dts.Runtime;
using System.Windows.Forms;
using System.Diagnostics;
using System.IO;
#endregion

namespace ST_ede13bf63ef441cb8d641613fa86bbda
{
	[Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
	public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
	{
        public void Main()
        {
            try
            {
                String connectionString = (String)Dts.Variables["User::connectionString"].Value;
                String reportOutputDir = (String)Dts.Variables["User::reportOutputDir"].Value;
                String reportRemoteOutputDir = (String)Dts.Variables["User::reportRemoteOutputDir"].Value;

                UploadLocalFile(connectionString, reportOutputDir, reportRemoteOutputDir);

                Dts.Variables["User::emailSubject"].Value = "Reporte SIG cargado en el ftp";
                Dts.Variables["User::emailBody"].Value = "Un saludo cordial.";

                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
                try
                {
                    using (StreamWriter sw = File.AppendText(@"C:\SSIS_Configuration\LoadDataDailyPackage.log"))
                    {
                        sw.WriteLine(String.Format("{0} Upload report error: {1}", DateTime.Now.ToString(@"hh\:mm\:ss\:fff"), ex.Message));
                    }

                    Dts.TaskResult = (int)ScriptResults.Success;
                }
                catch (Exception ex1)
                {
                    Dts.TaskResult = (int)ScriptResults.Failure;
                }
            }
        }

        static void UploadLocalFile(String connetionStr, String reportOutputDir, String reportRemoteOutputDir)
        {
            var now = DateTime.Now;
            var date = DateTime.Parse(now.AddDays(now.DayOfWeek == DayOfWeek.Monday ? -3 : -1).ToString("dd/MM/yyyy"));
            var path = String.Format("{0}{1}{2}",
                                   reportOutputDir,
                                   reportOutputDir.EndsWith("\\") ? "" : "\\",
                                   String.Format("10_{0}.txt", date.ToString("yyyyMMdd")));

            var proc = new Process
            {
                StartInfo = new ProcessStartInfo
                {
                    FileName = "winscp.com",
                    Arguments = String.Format("/command \"open sftp://{0}\" \"put -nopreservetime -nopermissions \"\"{1}\"\" \"\"{2}/\"\"\" \"exit\"",
                    connetionStr,
                    path,
                    reportRemoteOutputDir),
                    UseShellExecute = false,
                    RedirectStandardOutput = true,
                    CreateNoWindow = true
                }
            };

            proc.Start();
            proc.WaitForExit();
        }

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
        /// This code was generated automatically.
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

	}
}]]></ProjectItem>
